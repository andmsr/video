/* pages/search/search.wxss */
.f14{
  font-size: 14px;
}
.fc3{
  color: #333;
}
.hidden{
  display: none;
}
.search-input-box{
  width: 100%;
  text-align: center;
  position: fixed;
  background-color: #fff;
  top:0;
  height: 30px;
}
.search-input input{
  width: 70%;
  border: 1px solid rgba(0, 0, 0, 0.3);
  margin-left: 3%; 
  border-radius: 15px;
  height:23px;
  line-height:23px;
  float: left;
}
.search-input-box .search-input-btn{
  width: 20%;
  float: left;
}
.search-list{
  margin-top:30px;
}
.infolist-img,.score-left{
  width: 35%;
  float: left; 
  height: 167px;
  margin: 5px;
}
.infolist-content,.score-right{
  width: 59%;
  float: left; 
  height: 167px;
  margin: 5px;
}
.infolist-img image,.score-left image{
  width: 100%;
  height: 100%
}
.infolist-content-name,.score-name{
  font-size: 16px;
  color: #333333;
  height: 30px;
  line-height: 30px;
  overflow: hidden; 
}
.infolist-content-resolution,.score-resolution{
  color: #666;
  font-size: 14px;
}
.infolist-content-starting,.infolist-content-direc,.score-content{
  width: 100%;
  height: 30px;
  line-height: 30px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.line-style{
  width: 100%;
  border-top:1px solid rgba(0, 0, 0, 0.1); 
  float: left; 
}
.float-btn-box{
  position: fixed;
  top: 50%;
  right: 0;
  /* background-color: rgba(0, 0, 0, 0.1); */
  /* color: #fff; */
  width: 32px;
  height: 32px;
  line-height: 32px;
}
.float-btn-box image{
  width: 32px;
  height: 32px
}
.f20{
  width: 20%;
  float: left;
  height: 30px;
  line-height: 30px;
}
.f80{
  width: 80%;
  float: left;
  overflow: hidden;
  color:#C60;
  height: 30px;
  line-height: 30px;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.score-info-list{
  width: 100%;
}
.score-list{
  margin-top: 30px;
}
.top-search-btn{
  width:20%;
  border:1px solid rgba(0, 0, 0, 0.3);
  margin-left:1px; 
  border-radius:15px;
  height:23px;
  line-height:23px;
  float:left;
  color: #666
}